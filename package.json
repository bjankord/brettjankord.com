{
  "name": "brettjankord.com",
  "description": "Brett Jankord's Personal Website",
  "version": "1.0.0",
  "author": "Brett Jankord",
  "homepage": "https://brettjankord.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjankord/brettjankord.com.git"
  },
  "main": "n/a",
  "scripts": {
    "blog": "hygen blog new",
    "build": "gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "license-check": "license-checker --exclude 'MIT, MIT OR X11, ISC, Apache-2.0, BSD, WTFPL'",
    "lint:md": "textlint 'content/blog/**'",
    "lint-fix:md": "textlint --fix 'content/blog/**'",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.3.3",
    "gatsby-image": "^2.0.35",
    "gatsby-paginate": "^1.0.16",
    "gatsby-plugin-collections": "^1.1.1",
    "gatsby-plugin-feed": "^2.1.0",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.25",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "reading-time": "^1.1.3"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "hygen": "^3.0.0",
    "license-checker": "^25.0.0",
    "prettier": "^1.14.2",
    "slugify": "^1.3.4",
    "textlint": "^11.2.1",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-no-dead-link": "^4.4.1",
    "textlint-rule-no-start-duplicated-conjunction": "^2.0.2",
    "textlint-rule-terminology": "^1.1.30"
  }
}
